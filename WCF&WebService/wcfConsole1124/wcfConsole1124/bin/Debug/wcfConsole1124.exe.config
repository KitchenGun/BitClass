<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.serviceModel>
    <services>
      <!-- 네임스페이스명을 포함한 서비스 객체 -->
      <service name="wcfConsole1124.HelloWorldWCFService"
               behaviorConfiguration="1111111">
        <!-- behavior에 정의된 key -->
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8080/wcf/example/helloworldservice"/>
            <add baseAddress="net.tcp://localhost:8081/wcf/example/helloworldservice"/>
          </baseAddresses>
        </host>
        <endpoint
          contract ="wcfConsole1124.IHelloWorld"
          address =""
          binding ="basicHttpBinding"
          />
        <endpoint
          contract="wcfConsole1124.IHelloWorld"
          address =""
          binding ="netTcpBinding"
          />
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="1111111">
          <!-- KEY -->
          <serviceMetadata httpGetEnabled="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>